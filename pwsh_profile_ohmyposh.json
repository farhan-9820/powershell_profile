            "style": "plain",
            "type": "time"
          },
          {
            "type": "upgrade",
            "style": "plain",
            "foreground": "#111111",
            "background": "#FFD664",
            "properties": {
              "cache_duration": "168h"
            }
          }, 
  
         {
            "type": "battery",
            "style": "powerline",
            "invert_powerline": true,
            "powerline_symbol": "ÓÇ≤",
            "foreground": "#ffffff",
            "background": "#8f43f3",
            "properties": {
              "discharging_icon": "<#ffa500>-</> ",
              "charging_icon": "+ ",
              "charged_icon": "* "
            }
          },
         
